{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "seeders": {
    "type": "application",
    "root": "libs/seeders",
    "entryFile": "main",
    "sourceRoot": "libs/seeders/src",
    "compilerOptions": {
      "webpack": false,
      "tsConfigPath": "libs/seeders/tsconfig.app.json"
    }
  }
}
